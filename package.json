{
  "name": "gulp-front-matter",
  "version": "1.1.0",
  "description": "Extract front-matter header from files",
  "repository": "lmtm/gulp-front-matter",
  "files": [
    "index.js"
  ],
  "scripts": {
    "pretest": "jscs *.js test/*.js && eslint *.js test/*.js",
    "test": "_mocha",
    "coverage": "istanbul cover _mocha",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "engines": {
    "node": ">=0.9.0"
  },
  "keywords": [
    "gulpplugin",
    "yml",
    "yaml",
    "data",
    "property",
    "attribute",
    "attributes",
    "frontmatter",
    "front-matter",
    "meta",
    "meta-data",
    "gulpplugin"
  ],
  "author": "Nicolas Chambrier <naholyr@gmail.com> (http://naholyr.fr/)",
  "license": "BSD-2-Clause",
  "dependencies": {
    "front-matter": "^0.2.0",
    "gulp-util": "^3.0.1",
    "map-stream": "^0.1.0"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "eslint": "^0.10.2",
    "gulp": "^3.8.10",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.8.1",
    "mocha": "^2.0.1"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 128,
    "validateLineBreaks": "LF"
  }
}
